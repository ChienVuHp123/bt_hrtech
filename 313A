#include <stdio.h>
#include <math.h>
int main()
{
	int n;
	scanf("%d", &n);
	if (n >= 0) {
	   printf ("%d", n);
	}
	else
	    {
	     int number1, number2;     
	      number1 = n/10;
		  number2 = n/100*10+n%10;
		if ( number1 > number2)
		 {
			printf ("%d", number1);
		 } 
		else
		 {
			printf ("%d", number2);
		 }
	}
	return 0;
}
